{
  "name": "@unimatch/fetcher",
  "version": "1.0.0",
  "private": true,
  "description": "Fetches files from the Unicode Character Database.",
  "keywords": [
    "unicode",
    "parser",
    "ucd"
  ],
  "repository": "https://github.com/GregRos/parjs",
  "license": "MIT",
  "author": "GregRos <work.gregr@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "typings": "dist/index",
  "files": [
    "src",
    "dist",
    "examples",
    "package.json",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc -b .",
    "build:clean": "run-s clean build",
    "watch": "tsc -b . --watch",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "produce-source-map": true,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true
  },
  "dependencies": {
    "file-system-cache": "^2.4.4",
    "got": "^14.4.0",
    "parjs": "*",
    "pretty-ms": "^9.0.0",
    "roarr": "^7.21.1"
  },
  "devDependencies": {
    "@swc/core": "1.3.96",
    "@swc/jest": "0.2.29",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/unicode-trie": "^2.0.3",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.1.1",
  "engines": {
    "node": ">=16.0.0"
  }
}

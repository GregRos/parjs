{
  "name": "parjs",
  "version": "1.2.3",
  "repository": "https://github.com/GregRos/parjs",
  "homepage": "https://github.com/GregRos/parjs",
  "exports": {
    ".": {
      "require": "./dist/lib/index.js",
      "import": "./dist/lib/index.js",
      "default": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./combinators": {
      "require": "./dist/lib/combinators.js",
      "import": "./dist/lib/combinators.js",
      "default": "./dist/lib/combinators.js",
      "types": "./dist/lib/combinators.d.ts"
    },
    "./internal": {
      "require": "./dist/lib/internal.js",
      "import": "./dist/lib/internal.js",
      "default": "./dist/lib/internal.js",
      "types": "./dist/lib/internal.d.ts"
    },
    "./utilities": {
      "require": "./dist/utilities/index.js",
      "import": "./dist/utilities/index.js",
      "default": "./dist/utilities/index.js",
      "types": "./dist/utilities/index.d.ts"
    }
  },
  "typings": "dist/lib/index",
  "description": "A parser-combinator library for JavaScript.",
  "main": "dist/lib/index.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc",
    "clean": "shx rm -rf dist",
    "rebuild": "run-s clean build",
    "docs": "typedoc --out docs",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint --fix src/**/*.ts",
    "lint:fix": "run-s eslint:fix prettier:fix",
    "lint:check": "run-s -c prettier eslint",
    "prettier:fix": "prettier --write .",
    "prettier": "prettier --check .",
    "prepare": "husky"
  },
  "keywords": [
    "parser",
    "combinator",
    "parsing",
    "text",
    "string",
    "parser combinator",
    "parser-combinator",
    "functional"
  ],
  "author": "Greg Rosenbaum",
  "license": "MIT",
  "dependencies": {
    "char-info": "0.3.2"
  },
  "devDependencies": {
    "@gregros/eslint-config": "0.4.0",
    "@swc/core": "1.3.96",
    "@swc/jest": "0.2.29",
    "@types/jest": "29.5.8",
    "@types/node": "20.9.1",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.54.0",
    "eslint-plugin-jest": "27.6.0",
    "husky": "^9.0.10",
    "jest": "30.0.0-alpha.2",
    "lint-staged": "^15.2.2",
    "madge": "^6.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "3.2.4",
    "shelljs": "0.8.5",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "typedoc": "0.25.3",
    "typedoc-plugin-internal-external": "2.2.0",
    "typescript": "5.2.2"
  },
  "typesVersions": {
    "*": {
      "combinators": [
        "./dist/lib/combinators.d.ts"
      ],
      "internal": [
        "./dist/lib/internal.d.ts"
      ]
    }
  },
  "nyc": {
    "produce-source-map": true,
    "include": [
      "dist/lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "package.json",
    "package-lock.json",
    "tsconfig.json",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "packageManager": "yarn@4.1.0",
  "lint-staged": {
    "*.{js,css,md,ts,html,htm,yaml,yml,json}": "prettier --write"
  }
}

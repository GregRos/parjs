{
  "compilerOptions": {
    "composite": true,
    "module": "NodeNext",
    "noImplicitOverride": true,
    "target": "ES2020",
    "lib": ["ES2022"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "declarationMap": true,
    "sourceMap": true,
    "noEmit": true,
    "strict": true
  },
  "include": []
}

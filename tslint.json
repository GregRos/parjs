{
  "extends": [
    "tslint-config-gregros"
  ],
  "rules": {
    "completed-docs": {
      "severity": "warn",
      "options": {
        "functions": {
          "visibilities": ["exported"]
        },
        "interfaces": {
          "visibilities": ["exported"]
        },
        "classes": {
          "visibilities": ["exported"]
        },
        "namespaces": {
          "visibilities": ["exported"]
        },
        "types": {
          "visibilities": ["exported"]
        }
      }
    }
  }
}

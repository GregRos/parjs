{
  "name": "unicode-info",
  "version": "1.0.0",
  "private": true,
  "description": "An explorable object model of the Unicode Character Database.",
  "license": "MIT",
  "author": "GregRos <work.gregr@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "typings": "dist/index",
  "scripts": {
    "build": "tsc -b .",
    "build:clean": "run-s clean build",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch": "tsc -b . --watch"
  },
  "dependencies": {
    "@unicode-info/parser": "*",
    "file-system-cache": "^2.4.4",
    "got": "^14.4.1",
    "parjs": "*",
    "preszr": "^2.1.0",
    "pretty-ms": "^9.0.0",
    "roarr": "^7.21.1",
    "stdlazy": "^1.0.6",
    "stdseq": "^1.2.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.96",
    "@swc/jest": "0.2.29",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "typescript": "^5.5.2"
  },
  "packageManager": "yarn@4.1.1",
  "engines": {
    "node": ">=16.0.0"
  }
}

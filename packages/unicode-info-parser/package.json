{
  "name": "@unicode-info/parser",
  "version": "0.1.0",
  "description": "Parsers for Unicode Character Database (UCD) files using Parjs.",
  "license": "MIT",
  "author": "GregRos <work.gregr@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "typings": "dist/index",
  "scripts": {
    "build": "tsc -b .",
    "build:clean": "run-s clean build",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch": "tsc -b . --watch"
  },
  "dependencies": {
    "parjs": "*",
    "stdlazy": "^1.0.6",
    "stdseq": "^1.2.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.96",
    "@swc/jest": "0.2.29",
    "jest": "^29.7.0",
    "typescript": "^5.5.2"
  },
  "packageManager": "yarn@4.1.1",
  "engines": {
    "node": ">=16.0.0"
  }
}
